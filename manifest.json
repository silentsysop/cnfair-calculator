{
  "manifest_version": 3,
  "name": "Cnfair 80% calculator",
  "version": "1.1",
  "description": "Automatically calculates 80% off a product\nMade by silentsysop (discord)",
  "permissions": [
    "storage",
    "notifications"
  ],
  "host_permissions": [
    "https://cnfair.com/*"
  ],
  "action": {
    "default_title": "Toggle calculator Divider",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://cnfair.com/*"],
      "js": ["content.js"]
    }
  ]
}
